<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <data noupdate="0">
        <record id="exec_sh_backups" model="ir.cron">
            <field name="name">SH Backup</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="user_id" ref="base.user_root" />
            <field name="state">code</field>
            <field name="model_id" ref="model_sh_backup" />
            <field name="code">model.backup_database()</field>
            <field name="active" eval="True" />
        </record>
        <record id="exec_sh_backups_interval_tree" model="ir.cron">
            <field name="name">SH Backup Interval Tree Cleaner</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="user_id" ref="base.user_root" />
            <field name="state">code</field>
            <field name="model_id" ref="model_sh_backup" />
            <field name="code">model.clean_interval_tree()</field>
            <field name="active" eval="True" />
        </record>
        <record id="exec_sh_backups_google_drive" model="ir.cron">
            <field name="name">SH Backup Goole Drive</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="user_id" ref="base.user_root" />
            <field name="state">code</field>
            <field name="model_id" ref="model_sh_backup" />
            <field name="code">model.cron_storage()</field>
            <field name="active" eval="True" />
        </record>
    </data>
</odoo>